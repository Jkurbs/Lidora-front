{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "web-build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "appAssociation": "AUTO",
  "rewrites": [
    {
      "source": "/user/**",
      "dynamicLinks": true
    }
  ]
}