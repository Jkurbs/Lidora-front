{
  "expo": {
    "name": "Lidora",
    "slug": "Lidora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon/icon.png",
    "entryPoint": "./src/pages/home/Home.js",
    "splash": {
      "image": "./src/assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kurbs-lidora.com"
    },
    "android": {
      "package": "io.expo.client"
    },
    "web": {
      "favicon": "./src/assets/icon/favicon.png"
    }
  }
}
